@article{BerroneE2VEM,
title = {A first-order stabilization-free Virtual Element Method},
journal = {Applied Mathematics Letters},
volume = {142},
pages = {108641},
year = {2023},
issn = {0893-9659},
doi = {https://doi.org/10.1016/j.aml.2023.108641},
